{
  "testEnvironment": "node",
  "testRegex": "/test/.*\\.spec\\.ts",
  "transform": {
    "^.+\\.[jt]sx?$": "ts-jest"
  },
  "setupFilesAfterEnv": [
    "<rootDir>/test/jest.setup.ts"
  ],
  "moduleDirectories": [
    "node_modules",
    "<rootDir>/src"
  ],
  "moduleNameMapper": {
    "^react-native-css-parser$": "<rootDir>/src"
  },
  "coveragePathIgnorePatterns": [
    ".vscode",
    "node_modules/",
    "dist/"
  ],
  "globals": {
    "__DEV__": true
  }
}